  Di                       = {10.1080/10256010500229942},
  Em                       = {leuenberger@climate.unibe.ch},
  File                     = {:Huber2005.pdf:PDF},
  Ga                       = {974RP},
  J9                       = {ISOT ENVIRON HEALTH STUD},
  Ji                       = {Isot. Environ. Health Stud.},
  Keywords                 = {ABRUPT CLIMATE-CHANGE; ANTARCTIC ICE CORE; MASS-SPECTROMETRY; ATMOSPHERIC CO2; POLAR ICE; CENTRAL GREENLAND; OXYGEN-ISOTOPE; TRAPPED AIR; AGE SCALE; GRIP ICE},
