  Publisher                = {Amer Chemical Soc},
  Rp                       = {Baker, L (reprint author), Open Univ, Planetary & Space Sci Res Inst, Walton Hall, Milton Keynes MK7 6AA, Bucks, England.},
  Sc                       = {Chemistry},
