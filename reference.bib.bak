@Article{Barkan2005,
  Title                    = {High precision measurements of O-17/O-16 and O-18/O-16 ratios in H2O},
  Author                   = {Barkan, E. and Luz, B.},
  Journal                  = {Rapid Communications In Mass Spectrometry},
  Year                     = {2005},
  Number                   = {24},
  Pages                    = {3737--3742},
  Volume                   = {19},

  Abstract                 = {We have optimized the method of water fluorination using the solid reagent CoF3 to produce O-2. This allows isotope ratio measurements by dual-inlet mass spectrometry with very high precision of 0.01 to 0.03 parts per thousand for both delta(17)O and delta(18)O. Using this method, delta(17)O and delta(18)O of atmospheric O-2 were determined as 12.08 and 23.88 parts per thousand vs. VSMOW, respectively. Likewise, delta(17)O and delta(18)O of GISP were -13.12 and -24.73 parts per thousand, and for SLAP they were -29.48 and -55.11 parts per thousand vs. VSMOW, respectively. Analysis of these data in a ln(delta(17)O + 1) vs. ln(delta(18)O+ 1) plot yields a line with a regression coefficient (lambda) of 0.5279 +/- 0.0001 (R-2 = 0.999999). We also determined the fractionation factors 17 alpha and 18 alpha in liquid-vapor equilibrium, and found that the ratio In 17 alpha/ln(1) 8 alpha is constant (0.529 +/- 0.001) over the temperature range 11.4 to 41.5 degrees C. Copyright (c) 2005 John Wiley & Sons, Ltd.},
  C1                       = {Hebrew Univ Jerusalem, Inst Earth Sci, IL-91904 Jerusalem, Israel.},
  Doi                      = {10.1002/rcm.2250},
  Em                       = {eugenib@cc.huji.ac.il},
  File                     = {:Barkan2005.pdf:PDF},
  Ga                       = {996ET},
  J9                       = {RAPID COMMUN MASS SPECTROM},
  Ji                       = {Rapid Commun. Mass Spectrom.},
  Keywords                 = {TRIPLE-ISOTOPE COMPOSITION; ATMOSPHERIC OXYGEN; MASS-SPECTROMETRY; CARBON-DIOXIDE; WATER SAMPLES; FRACTIONATION; DELTA-O-17; SYSTEM; AIR; CO2},
  La                       = {English},
  Nr                       = {30},
  Owner                    = {vasilis},
  Pa                       = {THE ATRIUM, SOUTHERN GATE, CHICHESTER PO19 8SQ, W SUSSEX, ENGLAND},
  Pg                       = {6},
  Pi                       = {CHICHESTER},
  Publisher                = {John Wiley \& Sons Ltd},
  Rp                       = {Barkan, E, Hebrew Univ Jerusalem, Inst Earth Sci, IL-91904 Jerusalem,EOLEOLIsrael.},
  Sc                       = {Biochemical Research Methods; Chemistry, Analytical; Spectroscopy},
  Sn                       = {0951-4198},
  Tc                       = {42},
  Timestamp                = {2011.04.11},
  Ut                       = {ISI:000234157900019}
}

