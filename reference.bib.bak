@Article{Severinghaus1998,
  Title                    = {Timing of abrupt climate change at the end of the Younger Dryas interval from thermally fractionated gases in polar ice},
  Author                   = {Severinghaus, J. P. and Sowers, T. and Brook, E. J. and Alley, R. B. and Bender, M. L.},
  Journal                  = {Nature},
  Year                     = {1998},

  Month                    = jan,
  Number                   = {6663},
  Pages                    = {141--146},
  Volume                   = {391},

  Abstract                 = {Rapid temperature change fractionates gas Isotopes in unconsolidated snow, producing a signal that is preserved in trapped air bubbles as the snow forms ice, The fractionation of nitrogen and argon isotopes at the end of the Younger Dryas cold interval, recorded in Greenland ice, demonstrates that warming at this time was abrupt. This warming coincides with the onset of a prominent rise in atmospheric methane concentration, indicating that the climate change was synchronous (within a few decades) over a region of at least hemispheric extent, and providing constraints on previously proposed mechanisms of climate change at this time, The depth of the nitrogen-isotope signal relative to the depth of the climate change recorded in the Ice matrix indicates that, during the Younger Dryas, the summit of Greenland was 15 +/- 3 degrees C colder than today.},
